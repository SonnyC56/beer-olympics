    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.30",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "^3.2.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
