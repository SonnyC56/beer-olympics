{
  "name": "beer-olympics-app",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:api": "vercel dev --listen 3002",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "node scripts/test-all-e2e.js --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:puppeteer": "jest --config=jest.config.puppeteer.js",
    "test:puppeteer:headed": "HEADLESS=false jest --config=jest.config.puppeteer.js",
    "test:puppeteer:smoke": "jest --config=jest.config.puppeteer.js tests/puppeteer/smoke.test.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:puppeteer",
    "test:e2e:comprehensive": "node scripts/test-all-e2e.js",
    "test:e2e:smoke": "node scripts/test-all-e2e.js --smoke-only",
    "test:couchbase": "node scripts/test-couchbase.js",
    "test:user": "node scripts/test-user-creation.js",
    "view:labels": "node scripts/view-labeled-documents.js",
    "diagnose:couchbase": "node scripts/diagnose-couchbase.js",
    "init:couchbase": "node scripts/init-couchbase.js",
    "reset:couchbase": "node scripts/reset-couchbase.js",
    "test:redis": "node scripts/test-redis.js",
    "cache:warm": "ts-node src/utils/cache-warmer.ts warm",
    "cache:stats": "ts-node src/utils/cache-warmer.ts stats",
    "cache:invalidate": "ts-node src/utils/cache-warmer.ts invalidate"
  },
  "dependencies": {
    "@auth/core": "^0.39.1",
