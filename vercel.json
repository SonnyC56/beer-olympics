{
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3.1.1"
    },
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.1.1"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}